CREATE DATABASE "DB2";

CREATE TABLE developers (
  developerID SERIAL      NOT NULL PRIMARY KEY,
  name        VARCHAR(50) NOT NULL,
  teamID      INT
);

CREATE TABLE teams (
  teamID    SERIAL      NOT NULL PRIMARY KEY,
  name      VARCHAR(50) NOT NULL,
  projectID INT
);

CREATE TABLE projects (
  projectID SERIAL      NOT NULL PRIMARY KEY,
  name      VARCHAR(50) NOT NULL
);